{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/swagger/v1/swagger.json",
      "UpstreamPathTemplate": "/api/swagger/v1/swagger.json",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "auth-service",
          "Port": 80
        }
      ]
    },
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "auth-service",
          "Port": 80
        }
      ]
    },
    {
      "DownstreamPathTemplate": "/api/profiles/{everything}",
      "UpstreamPathTemplate": "/api/profiles/{everything}",
      "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"],
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "profile-service",
          "Port": 80
        }
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://api-gateway:80"
  }
}